# Artframe Development Environment Variables
# Copy this file to .env in the project root and fill in your actual values

# Immich Configuration
IMMICH_API_KEY=your_immich_api_key_here
IMMICH_SERVER_URL=https://your-immich-server.com

# NanoBanana Configuration
NANOBANANA_API_KEY=your_nanobanana_api_key_here

# Development Settings
ARTFRAME_LOG_LEVEL=DEBUG
ARTFRAME_CONFIG_PATH=config/artframe-dev.yaml

# Optional: Raspberry Pi SSH Configuration
PI_HOST=pi@192.168.1.100
PI_USER=pi

# Optional: Custom cache and output directories
ARTFRAME_CACHE_DIR=/tmp/artframe_cache
ARTFRAME_OUTPUT_DIR=/tmp/artframe_dev